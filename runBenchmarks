cd benchmarks
export CLASSPATH=../../:$CLASSPATH
dirlist=$(ls)
for dir in $dirlist
do 
pushd $dir 
echo "=================== Running test" $dir "==================="
curTest=$(ls *.mini)
java Mini -dumpIL $curTest 
rc=$?
if [[ $rc != 0 ]]; then echo "Failed to compile" $curTest; fi
curEx=$(ls *.s)
gcc $curEx
rc=$?
if [[ $rc != 0 ]]; then echo "gcc failed to compile" $curEx; fi
./a.out < input > testOut
diff testOut output 
popd
done
